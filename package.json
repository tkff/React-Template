{
  "name": "react-web-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "analyze": "node scripts/check-optional.js rollup-plugin-visualizer && vite build --mode analyze",
    "storybook": "node scripts/check-optional.js @storybook/react-vite && storybook dev -p 6006",
    "storybook:build": "node scripts/check-optional.js @storybook/react-vite && storybook build",
    "e2e": "node scripts/check-optional.js @playwright/test && playwright test",
    "e2e:ui": "node scripts/check-optional.js @playwright/test && playwright test --ui",
    "convex:dev": "node scripts/check-optional.js convex && convex dev",
    "convex:deploy": "node scripts/check-optional.js convex && convex deploy",
    "prepare": "node scripts/prepare.js"
  },
  "dependencies": {
    "@sentry/react": "^8.48.0",
    "@tanstack/react-query": "^5.62.0",
    "axios": "^1.7.9",
    "clsx": "^2.1.1",
    "i18next": "^24.2.2",
    "i18next-browser-languagedetector": "^8.0.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.54.2",
    "react-i18next": "^15.4.1",
    "react-router-dom": "^7.1.1",
    "tailwind-merge": "^2.6.0",
    "zod": "^3.24.1",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@hookform/resolvers": "^3.9.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "jsdom": "^25.0.1",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.0.6",
    "vitest": "^2.1.8"
  },
  "optionalDependencies": {},
  "packageManager": "pnpm@9.15.0",
  "engines": {
    "node": ">=18.0.0"
  }
}
